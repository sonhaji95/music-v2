{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "no-underscore-dangle":"off",
        "camelcase":"off",
        "spaced-comment":"off",
        "linebreak-style": "off",
        "indent": "off",
        "eol-last": "off",
        "no-trailing-spaces": "off",
        "padded-blocks": "off",
        "object-curly-newline": "off",
        "newline-per-chained-call": "off"
    }
}
